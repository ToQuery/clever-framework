<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="io.github.toquery.framework.log.rest.dao.SysLogRepository">
    <insert id="save">
        insert into sys_log (id, biz_name, log_type, module_name, raw_data, target_data, user_id)
        VALUES (#{sysLog.id}, #{sysLog.bizName}, #{sysLog.logType}, #{sysLog.moduleName}, #{sysLog.rawData},
                #{sysLog.targetData}, #{sysLog.userId})
    </insert>


    <select id="getById" resultType="io.github.toquery.framework.log.rest.entity.SysLog">
        select *
        from sys_log
        where id = #{id}
    </select>
    <select id="query" resultType="io.github.toquery.framework.log.rest.entity.SysLog">
        select *
        from sys_log
    </select>
</mapper>
