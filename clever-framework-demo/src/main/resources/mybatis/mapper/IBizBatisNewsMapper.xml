<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.toquery.framework.demo.dao.IBizBatisNewsMapper">

    <select id="getByMyBatisName" resultType="com.toquery.framework.demo.entity.BizBatisNews">
        select *
        from biz_batis_news
        where `name` = #{name}
    </select>
    <select id="findByMyBatisName" resultType="com.toquery.framework.demo.entity.BizBatisNews">
        select *
        from biz_batis_news
